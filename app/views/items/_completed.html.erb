<div class ="row">
  <% completed.each do |item| %>
    <div class="col-md-12">
      <p>
        <%= item.name %>
        <small class="text-warning">
          will be deleted <%= distance_of_time_in_words_to_now(item.expires_at) %>
            <%= link_to "", user_item_uncomplete_path(current_user,item), class: 'glyphicon glyphicon-circle-arrow-up', method: :put %>
        </small>
      </p>
    </div>
  <% end %>
</div>