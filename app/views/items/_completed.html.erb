<div class ="row">
  <% completed.each do |item| %>
    <div class="col-md-12">
      <p id="item-<%= item.id %>" >
        <%= item.name %>
        <small class="text-warning">
          will be deleted <%= distance_of_time_in_words_to_now(item.expires_at) %>
            <%= link_to "", user_item_complete_path(current_user,item), class: 'glyphicon glyphicon-trash', method: :put %>
        </small>
      </p>
    </div>
  <% end %>
</div>